    "lit-element": "^2.3.1",
    "lottie-web": "^5.6.6"
  },
  "devDependencies": {
    "@types/jszip": "^3.1.7",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "1.18.2",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist/dotlottie-player.js",
    "dist/dotlottie-player.js.map"
  ],
  "keywords": [
    "dotlottie",
